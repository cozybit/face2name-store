<h2>Event Details</h2>

<p>
  <b>Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Start date:</b>
  <%= @event.not_before %>
</p>

<p>
  <b>End date:</b>
  <%= @event.not_after %>
</p>

<p>
  <b>Administrator password:</b>
  <%= @event.admin_password %>
</p>

<%= link_to 'Download Configuration', configuration_event_path(@event) %> |                        
<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
